
site_name: Nuclei Bug Bounty Setup
site_url: https://Cyber-ShieldAcademy.github.io/Nuclei-BugBounty-Setup/

theme:
  name: material
  logo: images/logo.png         # place your logo under docs/images/
  favicon: images/favicon.png   # optional
  palette:
    - media: "(prefers-color-scheme: light)"
      scheme: default
      primary: blue
      accent: indigo
    - media: "(prefers-color-scheme: dark)"
      scheme: slate
      primary: blue
      accent: indigo
  features:
    - navigation.tabs
    - navigation.top
    - search.highlight
    - search.suggest

nav:
  - Home: index.md
  - Setup: setup.md
  - Flow: flow.md
  - Templates: templates.md
  - Cheatsheet: cheatsheet.md
  - FAQ: faq.md

extra:
  social:
    - icon: fontawesome/brands/github
      link: https://github.com/Cyber-ShieldAcademy
    - icon: fontawesome/brands/patreon
      link: https://patreon.com/CyberShieldAcademy

